{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}
